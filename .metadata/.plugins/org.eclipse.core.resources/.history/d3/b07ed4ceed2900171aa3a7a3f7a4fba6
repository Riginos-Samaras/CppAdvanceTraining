#include <iostream>
using namespace std;

class Animal{
public:
	int eyes;
	int legs;
	virtual Animal():eyes(2),legs(4){


	}


};

class Cat: Animal{
public:
	int tail;
	Cat():tail(1){
	}

};

int main() {

	Animal cat,dog,zebra;

	Cat cat1;
	cat.eyes=2;
	dog=zebra=cat;

	cat.eyes=2;


	cout<<dog.eyes<<endl;
	cout<<cat.eyes<<endl;






	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
