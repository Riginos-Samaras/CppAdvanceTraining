#include <iostream>
#include <vector>
using namespace std;

int main() {
	vector<int> numbers(0);

	cout<<"Size" <<number.size()<<endl;

	int capacity=numbers.capacity();
	cout<<"Capacity: "<<capacity<<endl;

	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
