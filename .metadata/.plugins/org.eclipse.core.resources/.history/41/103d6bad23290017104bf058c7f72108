#include <iostream>
using namespace std;

#pragma packe(push,1)
struct Person{
	int age;
	double weight;
};
int main() {

	cout << sizeof(int)<<endl;
	cout << sizeof(double)<<endl;
	cout << sizeof(string)<<endl;
	cout << sizeof(Person)<<endl;
	return 0;
}
