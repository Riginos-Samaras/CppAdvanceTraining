#include <iostream>
using namespace std;

class Animal{
public:
	int eyes;
	int legs;
	Animal():eyes(2),legs(4){


	}


};

class Cat: public Animal{
public:
	int tail;
	Cat():tail(1){
	}

};

int main() {

	Animal cat,dog,zebra;

	Cat cat1;
	cat.eyes=2;
	dog=zebra=cat;

	cat.eyes=2;

	cout<<cat1.eyes<<endl;

	cat1.eyes=10;
	dog.eyes=5;
//	cat1=dog;
	dog=cat1;
	cout<<cat1.eyes<<endl;



	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
