#include <iostream>
#include <vector>
using namespace std;

int main() {
	vector<int> B(4,1);
	for(int i=0;B.size(); i++){
		cout<<B[i]<<endl;
	}

	vector<vector <int> > grid(3, vector<int>(4,7));

	for(int row=0; row<grid.size(); row++){
		for(int col=0; col<grid[row].size();col++){
			cout<<grid[row][col]<<flush;
		}
		cout<<endl;
	}


	vector<int> g2(15);
	cout<<g2.size();


	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
