#include <iostream>
using namespace std;

class Animal{
public:
	string eyes;
	int legs;
	Animal():eyes("No"),legs(0){


	}


};

int main() {
	int i,j,k;

	i=10;
	j=k=i;
	cout<<i<<j<<k;
	Animal cat,dog,zebra;

	cout<<cat.eyes;




	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
